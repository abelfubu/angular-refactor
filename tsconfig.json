{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2022",
		"outDir": "out",
		"baseUrl": "src",
		"paths": {
			"@commands/*": [
				"./commands/*"
			],
			"@code-actions/*": [
				"./code-actions/*"
			],
			"@models/*": [
				"./models/*"
			],
			"@tools/*": [
				"./tools/*"
			],
			"@guards/*": [
				"./guards/*"
			],
			"@utils/*": [
				"./utils/*"
			],
		},
		"lib": [
			"ES2022"
		],
		"sourceMap": true,
		"strict": true,
		"noUnusedParameters": true,
	}
}